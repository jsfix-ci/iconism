
@font-face {
    font-family: "<%= family %>";
    <%- src %>;
    font-display: swap;
}
<% if (selector) { %> <%= selector %>:before<% } else { %>
<%= tag %>[class^="<%=prefix%>-"]:before, <%= tag %>[class*=" <%=prefix%>-"]:before<% } %> {
                font-family: "<%= family %>" !important;
                font-weight: normal !important;
                 font-style: normal;
               font-variant: normal;
            text-decoration: none;
             text-transform: none;
                line-height: 1;
     -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
<% glyphs.forEach((glyph) => { %><% 
if (selector) { %> <%= selector %>.<%= prefix %>-<%= glyph.name %><% } else { %>
<%= tag %>.<%= prefix %>-<%= glyph.name %><% } %>:before { content: "\<%= glyph.unicode[0].codePointAt(0).toString(16) %>" } <% }); %>
