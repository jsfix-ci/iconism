
$font-<%= family %>: "<%= family %>";

$glyphs-<%= family %>: (
<% glyphs.forEach((glyph) => { %> "<%=glyph.name%>": "\<%=glyph.unicode[0].codePointAt(0).toString(16)%>",
<% }); %> );

@font-face {
    font-family: $font-<%= family %>;
    <%- src %>;
    font-display: swap;
}

@each $name, $unicode in $glyphs-<%= family %> { <% if (selector) { %> <%= selector %>.<%= prefix %>-#{$name} <% } else { %>
    <%= tag %>.<%= prefix %>-#{$name}<% } %>:before { content: "#{$unicode}" }
}